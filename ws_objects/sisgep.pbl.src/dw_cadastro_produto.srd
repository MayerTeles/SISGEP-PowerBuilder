 retrieve="  SELECT Produto.Cod_produto,   
         Produto.Nome,   
         Produto.Marca,   
         Produto.Descricao,   
         Produto.Tamanho,   
         Produto.Preco,   
	     Produto.Cor,
         Estoque.Quantidade  
    FROM Estoque,   
         Produto  
   WHERE ( Estoque.Cod_produto = Produto.Cod_produto ) 
	AND   Produto.Cod_produto = :cod_produto 
" update="Produto" updatewhere=1 updatekeyinplace=no arguments=(("cod_produto", number)) )
